# Copy this file to .env and fill in your own values
# NEVER commit .env to version control!

# Firebase Configuration (get from Firebase Console)
VITE_FIREBASE_API_KEY=your_firebase_api_key_here
VITE_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your_project_id
VITE_FIREBASE_STORAGE_BUCKET=your_project.firebasestorage.app
VITE_FIREBASE_MESSAGING_SENDER_ID=your_messaging_sender_id
VITE_FIREBASE_APP_ID=your_app_id
VITE_FIREBASE_MEASUREMENT_ID=G-XXXXXXXXXX

# Gemini API Key (get from Google AI Studio)
VITE_GEMINI_API_KEY=your_gemini_api_key_here

# OpenWeather API Key (get from openweathermap.org)
VITE_OPENWEATHER_API_KEY=your_openweather_api_key_here

# Mandi API Key (get from data.gov.in)
VITE_MANDI_API_KEY=your_mandi_api_key_here

# Razorpay Payment Gateway (get from https://dashboard.razorpay.com/app/keys)
# Use test keys (rzp_test_...) for development
# Use live keys (rzp_live_...) only in production after KYC verification
VITE_RAZORPAY_KEY_ID=your_razorpay_key_id_here
# SECRET should never be exposed in frontend - for server-side use only
# VITE_RAZORPAY_KEY_SECRET=your_razorpay_key_secret_here
